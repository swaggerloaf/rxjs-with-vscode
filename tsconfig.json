{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    // Setting the "baseUrl" to a different directory than "packages/" because otherwise
    // packages like the native "http" module are resolved to the Angular "http" package.
    "baseUrl": ".",
    // "declaration": true,
    // "downlevelIteration": true,
    // "experimentalDecorators": true,
    // "emitDecoratorMetadata": true,
    // "strict": true,
    // //"moduleResolution": "node",
    // "strictNullChecks": true,
    // "strictPropertyInitialization": true,
    "outDir": "dist",
    // "noImplicitAny": true,
    // "noFallthroughCasesInSwitch": true,
    "paths": {
      // "selenium-webdriver": ["./node_modules/@types/selenium-webdriver/index.d.ts"],
      "rxjs/*": ["./node_modules/rxjs/*"]
      // "@angular/*": ["./packages/*"],
      // "zone.js/*": ["./packages/zone.js/*"],
      // "e2e_util/*": ["./modules/e2e_util/*"]
    },
    // "rootDir": ".",
    "lib": [
      "es5",
      "dom",
      "es2015.promise",
      "es2015.collection",
      "es2015.iterable",
      "es2015.core",
      "es2017.object"
    ],
    // "skipDefaultLibCheck": true,
    // "skipLibCheck": true,
    // "target": "es6",
    "types": ["rxjs"]
  }
}
